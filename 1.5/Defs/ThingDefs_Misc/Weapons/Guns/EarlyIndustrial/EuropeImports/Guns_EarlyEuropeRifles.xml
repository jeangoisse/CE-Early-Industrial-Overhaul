	<!-- Martini Henry 1871, text imported CHECK STATS + SOUNDS 

			 AmmoSet = unused .577-450 cartridge from Gun Galore Core. testing.

-->

  <ThingDef ParentName="BaseHumanMakeableGun">
    <defName>Gun_martini_henry</defName>
    <label>Martini Henry</label>
    <description>The emblematic english service-rifle during the Victorian era. Used overseas in Africa or India the power of the Martini Henry has been well documented for stopping both big game and people alike.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Things/Weapons/Guns/EuropeImports/Rifles/WWEmartini_henry</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <statBases>
      <WorkToMake>10000</WorkToMake>
      <SightsEfficiency>1</SightsEfficiency>
      <ShotSpread>0.03</ShotSpread>
      <SwayFactor>1.58</SwayFactor>
      <Bulk>11.50</Bulk>
      <Mass>3.65</Mass>
      <RangedWeapon_Cooldown>1.27</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>60</Steel>
      <ComponentIndustrial>2</ComponentIndustrial>
      <WoodLog>20</WoodLog>
    </costList>
    <verbs>
      <li Class="CombatExtended.VerbPropertiesCE">
        <recoilAmount>2.74</recoilAmount>
        <verbClass>CombatExtended.Verb_ShootCE</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Bullet_577450MartiniHenry_FMJ</defaultProjectile>
        <warmupTime>1.4</warmupTime>
        <range>55</range>
        <soundCast>Shot_singleshotrifle</soundCast>
        <soundCastTail>GunTail_Heavy</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <weaponTags>
	  <li>SimpleGun</li>
	  <li>CE_AI_SR</li>
    </weaponTags>
    <comps>
      <li Class="CombatExtended.CompProperties_AmmoUser">
        <magazineSize>1</magazineSize>
        <reloadTime>4.35</reloadTime>
        <ammoSet>AmmoSet_577450MartiniHenry</ammoSet>
      </li>
      <li Class="CombatExtended.CompProperties_FireModes">
        <aiAimMode>AimedShot</aiAimMode>
      </li>
    </comps>
    <recipeMaker>
      <researchPrerequisites>
      	<li>CEFP_LeverAction</li>
      	<li>CEFP_BreechLoading2</li>
    	</researchPrerequisites>
    </recipeMaker>
